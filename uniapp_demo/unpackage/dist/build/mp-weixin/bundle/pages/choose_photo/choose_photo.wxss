page{background:#eee}.image-box{display:flex;flex-direction:row;flex-wrap:wrap;margin-top:2px}.image-box .image-item{border:1px solid #eee;border-radius:8px;width:62px;height:62px;margin-right:9.5px;margin-top:9.5px;display:flex;background-color:#fff;position:relative}.image-box .image-item .image-icon{border-radius:8px;width:100%;height:100%;display:block}.image-box .image-item .image-delete{color:red;position:absolute;top:-5px;right:-5px}