
.charts-box {
	  width: 100vw;
	  height: 100vw;
    position: relative;
}
.charts-legend {
    position: absolute;
    bottom: 0px;
    left: 10px;
    font-size: 10px;
}
.legend-item {
    display: inline-block;
    width: 15px;
    height: 10px;
    margin-right: 5px;
    background-color: #0D9FD8;
}
.points-box {
    position: absolute;
    width: 50px;
    height: 50px;
    color: #FFFFFF;
    font-size: 14px;
    text-align: center;
    background: url(data:image/png;base64,);
    cursor: pointer;
    background-size:100% auto;
    background-position: center;
    background-repeat: no-repeat;
    background-origin:content-box;
}

