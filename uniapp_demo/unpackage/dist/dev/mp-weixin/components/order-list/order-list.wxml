<view class="order-list"><block wx:for="{{list}}" wx:for-item="order" wx:for-index="index" wx:key="index"><view class="order-detail" id="{{'good'+good.id}}"><view class="detail-top"><view class="order-title">{{order.item_name}}</view><view class="order-type">{{order.type}}</view></view><u-divider vue-id="{{'3262b9e2-1-'+index}}" hairline="{{true}}" line-color="#166" bind:__l="__l"></u-divider><view class="order-name"><view class="order-name-text">{{"姓名: "+order.person_name}}</view><image class="sex-image" src="{{order.person_sex==1?'/static/images/home_man.png':'/static/images/home_women.png'}}" mode="widthFix"></image><view class="order-mobile">{{''+order.person_mobile}}</view></view><view class="order-text">{{"照料人:"+order.caregiver_name+''}}</view><view class="order-text">{{"服务时间: "+order.plan_at}}</view><view class="order-text">{{"服务地址: "+(order.area_name+order.address)}}</view><view class="order-text">{{"医废登记: "+(order.garbage_record?order.garbage_record:'暂无')}}</view><view class="order-care"><view class="order-text">评价患者:</view><u-rate bind:input="__e" vue-id="{{'3262b9e2-2-'+index}}" count="{{5}}" activeColor="#f0ad4e" inactiveColor="#666666" value="{{order.care_level}}" data-event-opts="{{[['^input',[['__set_model',['$0','care_level','$event',[]],[[['list','',index]]]]]]]}}" bind:__l="__l"></u-rate></view></view></block></view>